#include <bits/stdc++.h> 
vector<int> findSpans(vector<int> &price) {
    // Write your code here.
    int n = price.size();
    stack<int> s;
    vector<int> ans;
    for(int i=0;i<n;i++){
        while(!s.empty() && price[i] >= price[s.top()])
            s.pop();
        if(s.empty()) ans.push_back(i+1);
        else ans.push_back(i-s.top());
        s.push(i);
    }
    return ans;
}
